{
  "name": "semester-project-two",
  "version": "1.0.0",
  "description": "Auction site for semester project for Noroff",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "dev": "vite",
    "vite-build": "vite build",
    "vite-preview": "vite preview",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barlaek/semester-project-two.git"
  },
  "keywords": [
    "auction",
    "webauction",
    "trade",
    "sales"
  ],
  "author": "Bjørn Arne Læknes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/barlaek/semester-project-two/issues"
  },
  "homepage": "https://github.com/barlaek/semester-project-two#readme",
  "devDependencies": {
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "vite": "^3.2.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ]
  }
