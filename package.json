{
  "name": "semester-project-two",
  "version": "1.0.10",
  "description": "Auction site for semester project for Noroff",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-cli": "cypress run",
    "dev": "vite",
    "vite-build": "vite build",
    "vite-preview": "vite preview",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barlaek/semester-project-two.git"
  },
  "keywords": [
    "auction",
    "webauction",
    "trade",
    "sales"
  ],
  "author": "Bjørn Arne Læknes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/barlaek/semester-project-two/issues"
  },
  "homepage": "https://github.com/barlaek/semester-project-two#readme",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^10.7.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.1.6",
    "husky": "^8.0.2",
    "jest": "^29.2.0",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "tailwindcss": "^3.2.4",
    "vite": "^3.2.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  }
}
